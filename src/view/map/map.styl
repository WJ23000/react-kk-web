.map
    max-width 1200px
    margin 0 auto
    .grid-map
        position relative
        float left
        width 600px
        height 440px
        &:nth-child(1)
            background rgba(255,255,255,0.5)
        &:nth-child(2)
            background rgba(255,255,255,1)
        &:nth-child(3)
            background rgba(255,255,255,1)
        &:nth-child(4)
            background rgba(255,255,255,0.5)